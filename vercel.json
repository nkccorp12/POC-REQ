{
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "excludeFiles": "{.cache,*.cache,.local,models,data,**/*.bin,**/*.pt,**/*.onnx,**/*.npy,**/*.npz,**/*.h5,**/*.index,**/*.faiss,.sentence_transformers,.huggingface}/**"
    }
  },
  "env": {
    "STREAMLIT_SERVER_HEADLESS": "true",
    "STREAMLIT_BROWSER_GATHER_USAGE_STATS": "false",
    "STREAMLIT_SERVER_ENABLE_CORS": "false",
    "TRANSFORMERS_CACHE": "/tmp/.cache/transformers",
    "HF_HOME": "/tmp/.cache/huggingface",
    "SENTENCE_TRANSFORMERS_HOME": "/tmp/.cache/sentence_transformers"
  }
}